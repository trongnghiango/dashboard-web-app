  "scripts": {
    "start": "yarn serve",
    "build": "yarn build-ts && yarn next:build",
    "serve": "node dist/server.js",
    "watch-node": "nodemon dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\" \"yarn next:dev\"",
    "test": "jest --forceExit --coverage --verbose",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "prettier": "prettier --write '**/*.{ts,css,js}'",
    "next:lint": "next lint ./next",
    "next:dev": "next dev ./next -p 8888",
    "next:build": "next build ./next",
    "next:start": "next start ./next",
    "analyze": "cross-env ANALYZE=true next build ./next",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build ./next",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build ./next",
    "commit": "cz"
  },
